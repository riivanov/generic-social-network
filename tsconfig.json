{
  "compilerOptions": {
    "baseUrl": ".",
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2016"
  },
  "exclude": ["node_modules"]
  // "include": [
  //   "next-env.d.ts",
  //   ".next/types/**/*.ts",
  //   "**/*.ts",
  //   "**/*.tsx",
  //   "app/auth/login/page.tsx"
  // ]
}
