{
  "collectCoverageFrom": ["**/*.(t|j)s"],
  "coverageDirectory": "../coverage",
  "moduleFileExtensions": ["js", "json", "ts"],
  "moduleNameMapper": {
    "@lib/entity/*.ts": "<rootDir>/../../lib/"
  },
  "rootDir": "src",
  "testEnvironment": "node",
  "testRegex": ".*\\.spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
